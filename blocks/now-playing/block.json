{
	"apiVersion": 2,
	"title": "Now Playing",
	"name": "scrobbble/now-playing",
	"category": "theme",
	"icon": "playlist-audio",
	"editorScript": "file:./block.js",
	"editorStyle": "file:./block.css",
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "scrobbble",
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	}
}
